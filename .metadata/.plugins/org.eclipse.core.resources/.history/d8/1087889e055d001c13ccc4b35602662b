package com.ibm.model;

public class ErrorItem {
	private String code;
	private String msg;
	private String error;
	public ErrorItem() {}
	
	public ErrorItem(String code, String msg, String error) {
	
		this.code = code;
		this.msg = msg;
		this.error = error;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getMsg() {
		return msg;
	}
	public void setMsg(String msg) {
		this.msg = msg;
	}
	public String getError() {
		return error;
	}
	public void setError(String error) {
		this.error = error;
	}
	

}
